  mixin Hello {
   void hello(){
       for (int i = 0; i < 5; i++) {
      print('hello moto ${i + 1}');
     }  
    }   
  }
  
  mixin Bye {void saygoodbye() => print('Goodbye');}
  
   class Say with Hello {
    void sayhello(String nome){
      hello();
    }
  }

void main() {  
 var x = Say();
 x.hello();  
}
